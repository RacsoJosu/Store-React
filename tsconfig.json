{
  "files": [],
  "compilerOptions": {
    "types": ["vite/client"],

    "plugins": [{ "name": "typescript-plugin-css-modules" }],

    "esModuleInterop": true,

    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@pages/*": ["src/Pages/*"],
      "@routes/*": ["src/routes/*"],
      "@utils/*": ["src/utils/*"],
      "@core/*": ["src/Core/*"]
    }
  },
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ]
}
